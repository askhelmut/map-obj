{
  "name": "@atomic-app/map-obj",
  "version": "2.0.3",
  "description": "Browser-friendly fork of sindresorhus/map-obj",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/atomic-app/map-obj.git"
  },
  "author": {
    "name": "Tane Morgan",
    "url": "http://github.com/tanem"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && ava",
    "build": "babel --presets env --plugins transform-runtime index.js -d lib",
    "prepublish": "npm run build"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "map",
    "obj",
    "object",
    "key",
    "keys",
    "value",
    "values",
    "val",
    "iterate",
    "iterator",
    "rename",
    "modify",
    "deep",
    "recurse",
    "recursive"
  ],
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "ignore": [
      "lib/index.js"
    ]
  },
  "main": "lib/index.js",
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
